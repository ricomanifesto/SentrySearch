# SentrySearch Environment Configuration

# Database Configuration (AWS RDS PostgreSQL)
DB_HOST=your-rds-endpoint.amazonaws.com
DB_PORT=5432
DB_NAME=sentrysearch
DB_USER=postgres
DB_PASSWORD=your-secure-password
DB_DEBUG=false

# AWS Configuration
AWS_ACCESS_KEY_ID=your-access-key-id
AWS_SECRET_ACCESS_KEY=your-secret-access-key
AWS_REGION=us-east-1
AWS_S3_BUCKET=sentrysearch-reports

# Anthropic API
ANTHROPIC_API_KEY=your-anthropic-api-key

# Workers Configuration (existing)
WORKERS_URL=https://sentry-search-hybrid.your-domain.workers.dev
ML_RETRIEVER_MODE=auto

# Application Configuration
ENABLE_REPORT_STORAGE=true
ENABLE_TRACING=true
ENABLE_METRICS=true